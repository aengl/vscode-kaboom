  "activationEvents": [
    "onCommand:extension.insert",
    "onCommand:extension.replaceSelection"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.insert",
        "title": " ðŸ’¥ Kaboom!: Insert"
      },
      {
        "command": "extension.replaceSelection",
        "title": " ðŸ’¥ Kaboom!: Replace Selection"
      }
    ]
